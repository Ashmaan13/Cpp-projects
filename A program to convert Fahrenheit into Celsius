#include <iostream>
#include <cmath>

using namespace std;

int main() {

    cout << "Fahrenheit: ";
    double Fahrenheit;
    cin >> Fahrenheit;
    double Celsius = (Fahrenheit - 32) * 5.0 / 9.0; // Formula to convert Fahrenheit into Celsius
    cout << "Celsius: " << Celsius;
    
    return 0;
}
